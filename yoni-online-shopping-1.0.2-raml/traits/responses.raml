#%RAML 1.0 Trait
responses:
  404:
    body:
      application/json:
        example:
          {
            "message": "not found"
          }
  401:
      body:
        application/json:
          example:
            {
              "message": "unauthorized access"
            }
headers:
  userid:
    type: string
    required: true
  user_secret: 
    type: string
    required: true
queryParameters:
  user_unique_id:
    displayName: special id
    required: true



